<?php
     $a=5;
     $b=6;
     if ($a == $b) {
          echo 'A variável $a é igual a variável $b';
     }
     elseif (($a + $b) == 11){
          echo 'A soma das variáveis $a + $b é igual a 11';
     }
     else{
          echo 'A soma das variáveis $a + $b é diferente a 11';
     }
?>